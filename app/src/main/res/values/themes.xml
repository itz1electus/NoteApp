<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ROOMExample" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/magnolia</item>
        <item name="colorPrimaryVariant">@color/french_gray</item>
        <item name="colorOnPrimary">@color/battleship_gray</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/rose_quartz</item>
        <item name="colorSecondaryVariant">@color/wenge</item>
        <item name="colorOnSecondary">@color/wenge</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="TextAppearance.AppCompat.Widget.ActionBar.Title" parent="@style/TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
    </style>

</resources>
